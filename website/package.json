        "@types/eslint": "9.6.1",
        "autoprefixer": "10.4.20",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-svelte": "2.44.0",
        "eslint": "9.10.0",
        "globals": "15.9.0",
        "picomatch": "4.0.2",
        "postcss": "8.4.47",
        "prettier-plugin-svelte": "3.2.6",
        "prettier-plugin-tailwindcss": "0.6.6",
        "prettier": "3.3.3",
        "svelte-check": "4.0.2",
        "svelte": "4.2.19",
        "tailwindcss": "3.4.12",
        "typescript-eslint": "8.6.0",
        "typescript": "5.6.2",
        "vite": "5.4.6"
